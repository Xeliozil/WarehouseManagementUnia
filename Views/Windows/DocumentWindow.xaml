<Window x:Class="WarehouseManagementUnia.Views.Windows.DocumentWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Nowy Dokument" Height="450" Width="600">
    <Grid Margin="10">
        <StackPanel>
            <ComboBox x:Name="DocumentTypeComboBox" Margin="0,0,0,10" SelectedIndex="0">
                <ComboBoxItem Content="Dostawa"/>
                <ComboBoxItem Content="MM"/>
                <ComboBoxItem Content="Wydanie"/>
            </ComboBox>
            <ComboBox x:Name="WarehouseComboBox" Margin="0,0,0,10" PlaceholderText="Magazyn"/>
            <ComboBox x:Name="ContractorComboBox" Margin="0,0,0,10" PlaceholderText="Kontrahent"/>
            <Button Content="Generuj Dokument" Click="GenerateDocument_Click"/>
        </StackPanel>
    </Grid>
</Window>
